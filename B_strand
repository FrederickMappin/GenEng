string = "ATCGATACGA"

new_string = ""
for char in string:
    if char == 'A':
        new_string += 'T'
    elif char == 'T':
        new_string += 'A'
    elif char == 'C':
        new_string += 'G' 
    elif char == 'G':
        new_string += 'C'
    else:
        new_string += char
        
print(new_string)
